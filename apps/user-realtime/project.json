{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/user-realtime/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/user-realtime",
        "main": "apps/user-realtime/src/main.js",
        "tsConfig": "apps/user-realtime/tsconfig.app.json",
        "assets": ["apps/user-realtime/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/user-realtime/src/environments/environment.js",
              "with": "apps/user-realtime/src/environments/environment.prod.js"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "user-realtime:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "user-realtime:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/user-realtime/**/*.js"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/user-realtime"],
      "options": {
        "jestConfig": "apps/user-realtime/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
